{
  "compilerOptions": {
    "lib": ["dom"],
    "outDir": "build",
    "module": "esnext",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "downlevelIteration": true,
    "strictNullChecks": true
  },
  "exclude": ["node_modules"]
}
